                    </ol>

                    <nav class="product-pager ml-auto" aria-label="Product">
                        <a class="product-pager-link product-pager-prev" href="#" aria-label="Previous" tabindex="-1">
                            <i class="icon-angle-left"></i>
                            <span>Prev</span>
                        </a>

                        <a class="product-pager-link product-pager-next" href="#" aria-label="Next" tabindex="-1">
                            <span>Next</span>
                            <i class="icon-angle-right"></i>
                        </a>
                    </nav><!-- End .pager-nav -->
                </div><!-- End .container -->
            </nav><!-- End .breadcrumb-nav -->

            <div class="page-content">
                <div class="container">
                    <div class="product-details-top">
                        <div class="row">
                            <!-- Product Images Section -->
                            <div class="col-md-6">
                                <div class="product-gallery product-gallery-vertical">
                                    <div class="row">
                                        <figure class="product-main-image">
                                            <!-- Set the default image for the zoom feature -->
                                            <img id="product-zoom" class="img-thumbnail" 
                                                 src="<%= product.images[0] %>" 
                                                 data-zoom-image="<%= product.images[0] %>" 
                                                 alt="<%= product.productName %>">
                                            <a href="#" id="btn-product-gallery" class="btn-product-gallery">
                                                <i class="icon-arrows"></i>
                                            </a>
                                        </figure><!-- End .product-main-image -->
                            
                                        <div id="product-zoom-gallery" class="product-image-gallery">
                                            <% product.images.forEach((image, index) => { %>
                                                <a class="product-gallery-item <%= index === 0 ? 'active' : '' %>" 
                                                   href="#" 
                                                   onclick="changeImage('<%= image %>')">
                                                    <img src="<%= image %>" alt="<%= product.productName %>">
                                                </a>
                                            <% }) %>
                                        </div><!-- End .product-image-gallery -->
                                    </div><!-- End .row -->
                                </div><!-- End .product-gallery -->
                            </div><!-- End .col-md-6 -->
                            
                    
                            <!-- Product Details Section -->
                            <div class="col-md-6">
                                <div class="product-details">
                                    <h1 class="product-title"><%= product.productName %></h1><!-- End .product-title -->
                    
                                    <!-- Ratings -->
                                    <div class="ratings-container">
                                        <div class="ratings">
                                            <div class="ratings-val" style="width: 80%;"></div><!-- End .ratings-val -->
                                        </div><!-- End .ratings -->
                                        <a class="ratings-text" href="#product-review-link" id="review-link">( 2 Reviews )</a>
                                    </div><!-- End .rating-container -->
                    
                                    <!-- Price -->
                                    <div class="product-price">
                                        ₹<%= product.price %>
                                    </div><!-- End .product-price -->
                              
                                    <div>
                                        <% if (product.stockQuantity === 0) { %>
                                            <span style="color: red; font-weight: bold;">Out of Stock</span>
                                        <% } else { %>
                                            <%= product.stockQuantity %> in stock
                                        <% } %>
                                    </div>
                                    
                                    <!-- Description -->
                                    <div class="product-content">
                                        <p><%= product.description %></p>
                                    </div><!-- End .product-content -->
                    
                                   
                    
                                  
                    
                                    <div class="details-filter-row details-row-size">
                                        <label>Shape:</label>
                                        <p><%= product.shape %></p>
                                    </div>
                    
                                    <div class="details-filter-row details-row-size">
                                        <label>Thickness:</label>
                                        <p><%= product.thickness %> mm</p>
                                    </div>
                    
                                    <% if (product.strapDetails) { %>
                                    <div class="details-filter-row details-row-size">
                                        <label>Strap Material:</label>
                                        <p><%= product.strapDetails.material %></p>
                                    </div>
                    
                                    <div class="details-filter-row details-row-size">
                                        <label>Strap Width:</label>
                                        <p><%= product.strapDetails.width %> mm</p>
                                    </div>
                                    <% } %>
                    
                                    <!-- Quantity -->
                                    <!-- <div class="details-filter-row details-row-size">
                                        <label for="qty">Qty:</label>
                                        <div class="product-details-quantity">
                                            <input type="number" id="qty" class="form-control" value="1" min="1" max="10" step="1" data-decimals="0" required>
                                        </div>
                                    </div> -->
                    
                                    <!-- Actions -->
                                    <div class="product-details-action">
                                        <form action="/add-to-cart/<%= product._id %>" method="POST">
                                            <button type="submit" class="btn-product btn-cart">
                                                <span>Add to Cart</span>
                                            </button>
                                        </form>
                    
                                        <div class="details-action-wrapper">
                                            <a href="#" class="btn-product btn-wishlist" title="Wishlist"><span>Add to Wishlist</span></a>
                                            <a href="#" class="btn-product btn-compare" title="Compare"><span>Add to Compare</span></a>
                                        </div><!-- End .details-action-wrapper -->
                                    </div><!-- End .product-details-action -->
                    
                                    <!-- Footer (Categories & Social Share) -->
                                    <div class="product-details-footer">
                                        <div class="product-cat">
                                            <span>Category:</span>
                                            <a href="#">Watches</a>,
                                            <a href="#">Luxury</a>,
                                            <a href="#">Men's Watches</a>
                                        </div><!-- End .product-cat -->
                    
                                        <div class="social-icons social-icons-sm">
                                            <span class="social-label">Share:</span>
                                            <a href="#" class="social-icon" title="Facebook" target="_blank"><i class="icon-facebook-f"></i></a>
                                            <a href="#" class="social-icon" title="Twitter" target="_blank"><i class="icon-twitter"></i></a>
                                            <a href="#" class="social-icon" title="Instagram" target="_blank"><i class="icon-instagram"></i></a>
                                            <a href="#" class="social-icon" title="Pinterest" target="_blank"><i class="icon-pinterest"></i></a>
                                        </div>
                                    </div><!-- End .product-details-footer -->
                                </div><!-- End .product-details -->
                            </div><!-- End .col-md-6 -->
                        </div><!-- End .row -->
                    </div><!-- End .product-details-top -->
                    

                    <div class="product-details-tab">
                        <ul class="nav nav-pills justify-content-center" role="tablist">
                            <li class="nav-item">
                                <a class="nav-link active" id="product-desc-link" data-toggle="tab" href="#product-desc-tab" role="tab" aria-controls="product-desc-tab" aria-selected="true">Description</a>
                            </li>
                          
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane fade show active" id="product-desc-tab" role="tabpanel" aria-labelledby="product-desc-link">
                                <div class="product-desc-content">
                                    <h3>Product Information</h3>
                                      <!-- Product Details (e.g., color, size) -->
                                      <div class="details-filter-row details-row-size">
                                        <label>Warranty Period:</label>
                                        <p><%= product.warrantyPeriod %></p>
                                    </div>
                    
                                    <div class="details-filter-row details-row-size">
                                        <label>Water Resistance:</label>
                                        <p><%= product.waterResistance %></p>
                                    </div>
                    
                                    <div class="details-filter-row details-row-size">
                                        <label>Shape:</label>
                                        <p><%= product.shape %></p>
                                    </div>
                    
                                    <div class="details-filter-row details-row-size">
                                        <label>Thickness:</label>
                                        <p><%= product.thickness %> mm</p>
                                    </div>
                    
                                    <% if (product.strapDetails) { %>
                                    <div class="details-filter-row details-row-size">
                                        <label>Strap Material:</label>
                                        <p><%= product.strapDetails.material %></p>
                                    </div>
                    
                                    <div class="details-filter-row details-row-size">
                                        <label>Strap Width:</label>
                                        <p><%= product.strapDetails.width %> mm</p>
                                    </div>
                                    <% } %>
                             
                    </div><!-- End .product-details-tab -->

                   
                    <div class="related-products mt-5">
                        <h4>You may also like</h4>
                        <div class="row">
                            <% relatedProducts.forEach(relatedProduct => { %>
                                <div class="col-md-3 col-sm-6">
                                    <div class="card">
                                        <a href="/singleProduct/<%= relatedProduct._id %>">
