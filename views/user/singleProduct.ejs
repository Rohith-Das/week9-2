                                            <a href="/singleProduct/<%= relatedProduct._id %>" class="btn btn-primary">View Details</a>
                                        </div>
                                    </div>
                                </div>
                            <% }); %>
                        </div>
                    </div><!-- End .related-products -->
                </div><!-- End .container -->
            </div><!-- End .page-content -->
